<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                            version="2.0">

  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>

    <job>
      <name>TaskEmailNotification</name>
      <group>GroupOne</group>
      <description>Email notification</description>
      <job-type>NotificationService.Jobs.EmailNotification, NotificationService</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>


    <trigger>
      <cron>
        <name>EmailNotificationTrigger</name>
        <group>GroupOne</group>
        <description>Trigger every 2 minutes </description>
        <job-name>TaskEmailNotification</job-name>
        <job-group>GroupOne</job-group>
        <cron-expression>0 0/2 * 1/1 * ? *</cron-expression>
      </cron>
    </trigger>

  </schedule>

</job-scheduling-data>