@model Core.Reports.ViewModel.LocalMarketingViewModel

<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
        .fontClass {
            font-size: 16px !important;
        }

        body {
            padding: 0;
            margin: 0;
            font-size: 11px;
            font-family: none;
            page-break-after: avoid;
            page-break-before: avoid;
        }

        .emailtemplate {
            max-width: 900px;
            margin: 0 auto;
            align-content: center;
            align-items: center;
        }

        .emailheader {
            display: flex;
            align-items: center;
            height: 100px;
        }

        .heading {
            margin: 0;
            padding: 0;
            color: #808080;
            font-size: 16px;
            text-align: center;
            width: 100%;
        }

        .page {
            width: 99.9% !important;
            height: 99.9% !important;
        }

        html {
            height: 0;
        }

        table {
            page-break-after: avoid;
            page-break-before: avoid;
        }

            table tr td {
                padding: 5px 3px;
            }

        .widget {
            border: 1px solid #333;
            width: calc(100% - 40px);
            padding: 10px;
            margin: 20px auto;
        }

            .widget p {
                margin: 0;
            }

        .list-table tr th, .list-table tr td {
            text-align: left;
            vertical-align: top;
            padding: 3px 10px;
        }

        .table-single tr td {
            border-top: 1px solid #bec2c6;
            border-right: 1px solid #dee2e6;
            font-size: 9px;
        }

            .table-single tr td:last-child {
                border-left: 1px solid #bec2c6;
            }

        .table-single tr:last-child td {
            border-bottom: 1px solid #bec2c6;
        }

        .list-table tr th {
            background: #f4f5f7;
            border-top: 1px solid #dee2e6;
            font-size: 9px;
            height: 20px;
            border-right: 1px solid #dee2e6;
        }

        .list-table tr > th:first-child {
            border-left: 1px solid #dee2e6;
        }

        .list-table tr > td:first-child {
            border-left: 1px solid #dee2e6;
        }

        .list-table tr:last-child > td {
            border-bottom: 1px solid #dee2e6;
        }

        .list-table tr td {
            border-top: 1px solid #dee2e6;
            font-size: 9px;
            height: 20px;
            border-right: 1px solid #dee2e6;
        }

        .shape-list {
            list-style: none;
            margin: 0;
            padding: 0;
            margin-bottom: 3px;
        }

            .shape-list li {
                position: relative;
            }


        .flex-1 {
            flex: 1;
        }

        hr.rounded {
            border-top: 0px solid #bbb;
            border-radius: 5px;
            margin: 2px !important;
        }

        .is-best-image.is-add-to-galary-image {
            height: 377px !important;
        }

        .is-best-image.not-is-add-to-galary-image {
            height: 377px !important;
        }

        .numberCircle {
            border-radius: 50%;
            width: 60px;
            height: 41px;
            padding: 8px;
            background: #fff;
            border: 2px solid #666;
            color: #666;
            text-align: center;
            font: 14px Arial, sans-serif;
        }

        .img-class-commercial {
            width: 100%;
            margin: 0px;
            max-width: 100%;
            max-height: 300px;
        }

        .img-class-exterior {
            width: 100%;
            margin: 0px;
            max-width: 100%;
            max-height: 300px;
            object-fit: cover;
            object-position: center center;
        }

        .img-class {
            width: 100%;
            margin: 0px;
            max-width: 100%;
            max-height: 200px;
            object-fit: cover;
            object-position: center center;
        }

        .div-style {
            background: #e6e6e6;
            border: 2px solid #333;
        }

        .mbl-switch + label {
            position: relative;
            width: 70px;
            border: 1px solid #b9b9b9;
            border-radius: 4px;
            margin: 0;
            height: 22px;
            cursor: pointer;
        }

        .flex-1 {
            flex: 1;
        }

        .d-flex {
            display: flex;
        }

        .is-best-image {
            border: 5px solid lightblue;
            height: 377px !important;
        }

            .is-best-image.is-add-to-galary-image {
                height: 377px !important;
            }

            .is-best-image.not-is-add-to-galary-image {
                height: 377px !important;
            }

        .not-best-image {
            border: 0px solid lightblue;
        }

        .is-add-to-galary-image {
            border: 5px solid #26327e !important;
            /*        margin-bottom:36px !important;*/
        }

        .not-is-add-to-galary-image {
            border: 0px solid lightblue !important;
            /*        margin-bottom: 36px;*/
        }

        .mbl-switch2 {
            display: none;
        }

            .mbl-switch2 + label:after {
                position: absolute;
                width: 34px;
                content: 'No';
                background-color: #b9b9b9;
                top: 1px;
                color: #fff;
                font-size: 12px;
                height: 18px;
                text-align: center;
                border-radius: 3px;
                left: auto;
                right: 1px;
                width: 37% !important;
            }

            .mbl-switch2 + label {
                position: relative;
                width: 70px;
                border: 1px solid #b9b9b9;
                border-radius: 4px;
                margin: 0;
                height: 22px;
                cursor: pointer;
            }

            .mbl-switch2:checked + label:after {
                content: 'Yes';
                background-color: #578ebe;
                left: 1px;
                width: 37% !important;
            }

        .reviewed {
            background-color: green;
            color: white;
        }

        .notReviewed {
            background-color: red;
            color: white;
        }

        .enabled {
            background: lightblue !important;
        }

        .disabled {
            background: #fff;
        }

        .flex-1 {
            flex: 1;
        }

        .d-flex {
            display: flex;
        }

            .d-flex:hover {
                cursor: pointer;
            }

        .search-single {
            margin-right: 15px;
            display: inline-block;
            text-align: left;
        }

        .text-align {
            text-align: right;
        }
    </style>
</head>
<body style="margin: 0; padding: 0;">
    <div class="emailtemplate">
        <div class="emailheader">
            <table style="width:100%;" cellpadding="0" cellspacing="0">
                <tr>
                    <td><img src="data:image/png;base64," alt="logo" width="100" /></td>
                    <td></td>
                    <td>
                        <div>
                            @*<p style="margin:0;display:block;text-align:right;font-size:18px;">
                                    <img style="width:10px;height:10px" src="data:image/png;base64," /> @Model.CommunicationEmail
                                </p>*@
                        </div>
                        <div>
                            @*<p style="margin: 0; display: block; text-align: right; margin-right: 95px; font-size: 18px; margin-bottom: 20px !important">
                                    <img style="width:10px;height:10px" src="data:image/png;base64," /> @Model.PhoneNumber
                                </p>*@
                        </div>
                    </td>
                </tr>
                <tr>
                    <td colspan="3"></td>
                </tr>
                <tr>
                    <td colspan="3"><h5 style="text-align: left; margin-top: -20px; color: #26327e; font-family: 'kievit-book'; font-size: 20px; text-transform: uppercase; " class="heading">LOCAL MARKETING REVIEW FOR @Model.FranchiseeName</h5></td>
                </tr>
            </table>
        </div>
        <div class="emailbody">
            <p style="text-align: left; color: rgba(0,0,0,.87); font-family: 'kievit-book'; font-size: 17px; font-weight: bold; text-transform: uppercase; ">@Model.StartDate.ToString("MM/dd/yyyy") to @Model.EndDate.ToString("MM/dd/yyyy")</p>
            <br /><br />
            @foreach (var model2 in Model.BeforeAfterPersonsViewModal)
            {
                <b style="font-size:20px !important">@model2.PersonName ( @model2.PersonRole  )</b>
                if (model2.JobEstimateForBeforeAfter.Count() == 0)
                {
                    <p>No Job/Estimate Scheduled.</p>
                }
                else
                {
                    foreach (var model3 in model2.JobEstimateForBeforeAfter)
                    {
                        if (model3.BeforeAfterImageViewModal.Count() == 0)
                        {
                            <div style="padding:5px;">
                                <div style="float:right !important;margin-left: 100px;" class="numberCircle"><b>@model3.SchedulerNames</b></div>
                            </div>
                            <div>
                                <span style="margin-left: 5px;">
                                    <b>Before Image</b>
                                </span>
                                <span style="margin-left: 145px;">
                                    <b>After Image</b>
                                </span>
                            </div>
                            <span style="min-width: 50%; flex: 1; margin: 0px;">
                                @*<span style="margin:0"><b>Before Image</b></span>*@
                                <sapn style="background: #e6e6e6; border: 2px solid #fff; width: 200px !important; overflow: hidden; margin: 0px;">
                                    @if (model3.S3BucketBeforeImageUrlThumb != null)
                                    {
                                        <img style="height: 150px; width: 100%; max-width: 200px; border: 2px solid black; " class="w-100 img-class" src="@model3.S3BucketBeforeImageUrlThumb" />
                                    }
                                </sapn>
                            </span>
                            <span style="min-width: 50%; flex: 1; margin: 0px;">
                                @*<span style="margin:0"><b>After Image</b></span>*@
                                <span style="background: #e6e6e6; border: 2px solid #fff; width: 200px !important; overflow: hidden; margin: 0px;">
                                    @if (@model3.S3BucketAfterImageUrlThumb != null)
                                    {
                                        <img style="height: 150px; width: 100%; max-width: 200px; border: 2px solid black;" class="w-100 img-class" src="@model3.S3BucketAfterImageUrlThumb" />
                                    }
                                </span>
                            </span>
                            @*<p style="margin:0">
                            <span><b>Surface Color :</b></span><span></span>
                        </p>
                        <p style="margin:0">
                            <span><b>Surface Type :</b></span><span></span>
                        </p>
                        <p style="margin:0">
                            <span><b>Service Type :</b></span><span></span>
                        </p>*@
                            <p style="margin:0">
                                <span><b>Customer Name:</b></span><span>@model3.CustomerName</span>
                            </p>
                            if (@model2.PersonRole == "SalesRep")
                            {
                                <p style="margin:0">
                                    <span><b>Estimate Value:</b></span><span>@model3.EstimateValue</span>
                                </p>
                            }
                            <p style="margin:0">
                                <span><a href="@model3.SchedulerUrl">Click Here</a>To Change Images for Job/Estimate : @model3.Title</span>
                            </p>
                            <p style="margin:0">
                                <span><a href="@model3.LocalMarketingUrl">Click Here</a>To Mark The Image As Best Pair Or Add To Local Site Gallery</span>
                            </p>
                        }


                        foreach (var model4 in model3.BeforeAfterImageViewModal)
                        {
                            <p>@model4.MarketingClass</p>
                            <div style="background-color: #fff;padding: 10px;border: 0;">
                                @if (@model4.RelactiveLocationAfterImageUrlThumb != null)
                                {
                                    <div style="padding:5px;">
                                        <span>
                                            @if (@model4.IsBestPicture)
                                            {
                                                <input type="checkbox" checked /><span>Mark Best Pair</span>
                                            }
                                            @if (@model4.IsBestPicture == false)
                                            {
                                                <input type="checkbox" /><span>Mark Best Pair</span>
                                            }
                                        </span>
                                        <span>
                                            @if (@model4.IsAddToLocalGallery)
                                            {
                                                <input type="checkbox" checked /><span>Add to Local Site Gallery</span>
                                            }
                                            @if (@model4.IsAddToLocalGallery == false)
                                            {
                                                <input type="checkbox" /><span>Add to Local Site Gallery</span>
                                            }
                                        </span>
                                        <div style="float:right !important;margin-left: 100px;" class="numberCircle"><b>@model4.SchedulerNames</b></div>
                                    </div>
                                }
                            </div>
                            <div>
                                <span style="margin-left: 5px;">
                                    <b>Before Image</b>
                                </span>
                                <span style="margin-left: 145px;">
                                    <b>After Image</b>
                                </span>
                            </div>
                            <span style="display: flex !important; flex-wrap: wrap !important; margin: 0px; ">
                                <span style="min-width: 50%; flex: 1;">
                                    @*<span style="margin:0"><b>Before Image</b></span>*@
                                    <span class="text-center div-style" style="background: #e6e6e6; border: 2px solid #FFFFFF; width: 200px !important; overflow: hidden; margin: 0px; ">
                                        @if (@model4.RelactiveLocationBeforeImage != null)
                                        {
                                            <img style="border: 2px solid black; height: 150px; width: 100%; transform: @model4.BeforeCss; max-width: 200px;" class="w-100 img-class" src="@model4.RelactiveLocationBeforeImage" />
                                        }
                                        else
                                        {
                                            <img style="border: 2px solid black; height: 150px; width: 100%; transform: @model4.BeforeCss; max-width: 200px;" class="w-100 img-class" src="@model4.RelactiveLocationBeforeImageUrlThumb" />
                                        }
                                    </span>
                                </span>
                                <span class="flex-1" style="min-width: 50%; flex: 1; margin: 0px;">
                                    @*<span style="margin:0"><b>After Image</b></span>*@
                                    <span class="text-center div-style" style="background: #e6e6e6; border: 2px solid #FFFFFF; width: 200px !important; overflow: hidden; margin: 0px;">
                                        @if (@model4.RelactiveLocationAfterImage != null)
                                        {
                                            <img style="border: 2px solid black; height: 150px; width: 100%; transform: @model4.AfterCss; max-width: 200px;" class="w-100 img-class" src="@model4.RelactiveLocationAfterImage" />
                                        }
                                        else
                                        {
                                            <img style="border: 2px solid black; height: 150px; width: 100%; transform: @model4.AfterCss; max-width: 200px;" class="w-100 img-class" src="@model4.RelactiveLocationAfterImageUrlThumb" />
                                        }
                                    </span>
                                </span>
                            </span><br />
                            <div>
                                <span style="margin-right: 87px;"><b>Uploaded By :</b> @model4.BeforeImageUploadedBy</span><span><b>Uploaded By :</b> @model4.AfterImageUploadedBy</span>
                            </div>
                            <div>
                                <span style="margin-right: 87px; max-width: 120px !important"><b>Uploaded On :</b> @model4.BeforeImageUploadedOn</span><span> <b>Uploaded On :</b> @model4.AfterImageUploadedOn</span>
                            </div>
                            <p style="margin:0">
                                <span><b>Surface Color :</b></span><span> @model4.SurfaceColor</span>
                            </p>
                            <p style="margin:0">
                                <span><b>Surface Type :</b></span><span> @model4.SurfaceType</span>
                            </p>
                            <p style="margin:0">
                                <span><b>Service Type :</b></span><span>@model4.ServicesType</span>
                            </p>
                            <p style="margin:0">
                                <span><b>Customer Name:</b></span><span>@model4.CustomerName</span>
                            </p>
                            if (@model2.PersonRole == "SalesRep")
                            {
                                <p style="margin:0">
                                    <span><b>Estimate Value:</b></span><span>@model4.EstimateValue</span>
                                </p>
                            }
                            <p style="margin:0">
                                <span><a href="@model4.SchedulerUrl">Click Here</a>To Change Images for Job/Estimate : @model4.Title</span>
                            </p>
                            <p style="margin:0">
                                <span><a href="@model4.LocalMarketingUrl">Click Here</a>To Mark The Image As Best Pair Or Add To Local Site Gallery</span>
                            </p>
                        }
                    }
                }
                <br /><br />
            }
        </div>
    </div>

</body>
</html>