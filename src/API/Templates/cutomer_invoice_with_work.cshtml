<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
@model Core.Sales.ViewModel.EstimateInvoiceEditMailModel

<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
                .fontClass {
                    font-size: 16px !important;
                }

                body {
                    padding: 0;
                    margin: 0;
                    font-size: 11px;
                    font-family: none;
                    page-break-after: avoid;
                    page-break-before: avoid;
                }

                body, td, span, p, div {
                    white-space: normal !important;
                    word-spacing: 2px !important;
                }

                .emailtemplate {
                    max-width: 900px;
                    margin: 0 auto;
                    align-content: center;
                    align-items: center;
                }

                .emailheader {
                    display: flex;
                    align-items: center;
                    height: 100px;
                }

                .heading {
                    margin: 0;
                    padding: 0;
                    color: #808080;
                    font-size: 16px;
                    text-align: center;
                    width: 100%;
                }

                .page {
                    width: 99.9% !important;
                    height: 99.9% !important;
                }

                html {
                    height: 0;
                }

                /*table {
                    page-break-after: avoid;
                    page-break-before: avoid;
                }
        */

                table {
                    page-break-inside: auto;
                }

                    table tr {
                        page-break-inside: avoid;
                        page-break-after: auto;
                    }

                        table tr td {
                            padding: 5px 3px;
                            page-break-inside: avoid;
                            page-break-after: auto;
                        }


                    table thead {
                        display: table-header-group;
                    }

                .widget {
                    border: 1px solid #333;
                    width: calc(100% - 40px);
                    padding: 10px;
                    margin: 20px auto;
                }

                    .widget p {
                        margin: 0;
                    }

                .list-table tr th, .list-table tr td {
                    text-align: left;
                    vertical-align: top;
                    padding: 3px 10px;
                }

                .table-single tr td {
                    border-top: 1px solid #bec2c6;
                    border-right: 1px solid #dee2e6;
                    font-size: 9px;
                }

                    .table-single tr> td:last-child {
                        border-left: 1px solid #bec2c6;
                    }
                    .table-single tr> th:last-child {
                        border-left: 1px solid #bec2c6;
                    }

                .table-single tr:last-child> td {
                    border-bottom: 1px solid #bec2c6;
                }

        .table-single tr:last-child > th {
            border-bottom: 1px solid #bec2c6;
        }

                .list-table tr th {
                    background: #f4f5f7;
                    border-top: 1px solid #dee2e6;
                    font-size: 9px;
                    height: 20px;
                    border-right: 1px solid #dee2e6;
                }

                .list-table tr > th:first-child {
                    border-left: 1px solid #dee2e6;
                }

                .list-table tr > td:first-child {
                    border-left: 1px solid #dee2e6;
                }

                .list-table tr:last-child > td {
                    border-bottom: 1px solid #dee2e6;
                }

                .list-table tr td {
                    border-top: 1px solid #dee2e6;
                    font-size: 9px;
                    height: 20px;
                    border-right: 1px solid #dee2e6;
                }

                .shape-list {
                    list-style: none;
                    margin: 0;
                    padding: 0;
                    margin-bottom: 3px;
                }

                    .shape-list li {
                        position: relative;
                    }


                .flex-1 {
                    flex: 1;
                }
    </style>
</head>
<body style="margin: 0; padding: 0;">
    <div class="emailtemplate">
        <div class="emailheader">
            <table style="width:100%;" cellpadding="0" cellspacing="0">

                <tr>
                    <td><img src="data:image/png;base64," alt="logo" width="100" /></td>
                    <td></td>
                    <td>
                        <div>
                            <p style="margin:0;display:block;text-align:right;font-size:18px;">
                                <img style="width:10px;height:10px" src="data:image/png;base64," /> @Model.CommunicationEmail
                            </p>
                        </div>
                        <div>
                            <p style="margin:0;display:block;text-align:right;margin-right:95px;font-size:18px;margin-bottom:20px !important">
                                <img style="width:10px;height:10px" src="data:image/png;base64," /> @Model.PhoneNumber
                            </p>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td colspan="3"><h5 style="margin-top:0px" class="heading">CUSTOMER SERVICE AGREEMENT / WORK ORDER / INVOICE</h5></td>
                </tr>
            </table>
        </div>
        <div class="emailbody">
            <table style="width:100%;margin-top:10px" cellpadding="0" cellspacing="0">
                <tbody>
                    <tr>
                        <td style="padding: 0; vertical-align: top;">
                            <table style="width:100%;" cellpadding="0" cellspacing="0">
                                <tbody>
                                    <tr>
                                        <td class="fontClass"><b>Customer:</b></td>
                                        <td class="fontClass"> @Model.CustomerName</td>

                                    </tr>
                                    <tr>
                                        <td class="fontClass"><b>Street Address:</b></td>
                                        <td class="fontClass">@Model.Address</td>
                                    </tr>
                                    <tr>
                                        <td class="fontClass"><b>City, State  Zip:</b></td>
                                        <td class="fontClass">@Model.City</td>

                                    </tr>
                                    <tr>
                                        <td class="fontClass"><b>Phone 1:</b></td>
                                        <td class="fontClass">@Model.PhoneNumber1</td>
                                    </tr>
                                    <tr>
                                        <td class="fontClass"><b>Email:</b></td>
                                        <td class="fontClass">@Model.Email</td>
                                    </tr>
                                    <tr>
                                        <td class="fontClass">
                                            <b>Technician Name:</b>
                                        </td>
                                        <td class="fontClass">@Model.TechName</td>

                                    </tr>
                                    <tr>
                                        <td style="display: block;" class="fontClass">
                                            <b>Office Address:</b>
                                        </td>
                                        <td class="fontClass">@Model.OfficeName<br /> @Model.OfficeAddressLine1,@Model.OfficeAddressLine2<br />@Model.OfficeAddressCity, @Model.OfficeAddressState, @Model.OfficeAddressCountry - @Model.OfficeAddressZipCode</td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                        <td style="padding:0; vertical-align:top;width:40%">
                            <table style="width:100%;" cellpadding="0" cellspacing="0">
                                <tbody>
                                    <tr>
                                        <td class="fontClass">
                                            <b> Invoice Number:</b>
                                        </td>
                                        <td class="fontClass">@Model.InvoiceNumber  </td>

                                    </tr>
                                    <tr>
                                        <td class="fontClass">
                                            <b> Estimate Date:</b>
                                        </td>
                                        <td class="fontClass">@Model.EstimateDate  </td>

                                    </tr>
                                    <tr style="width:100%;margin-right:40px !important">
                                        <td class="fontClass">
                                            <b> Price for Service ($):</b>
                                            <p style="padding: 0; font-size: 10px; margin: 0;"><b>Based on @Model.ChooseOption</b></p>
                                        </td>
                                        <td class="fontClass"> $ @Model.Price</td>

                                    </tr>
                                    <tr>
                                        <td class="fontClass">
                                            <b>Less Deposit ($):</b>
                                            <p style="padding:0; font-size:8px; margin:0;"><b>@Model.LessDepositPer% at time of scheduling </b></p>
                                        </td>
                                        <td class="fontClass">$ @Model.LessDeposit</td>
                                    </tr>
                                    <tr>
                                        <td class="fontClass">
                                            <b>Balance on Completion ($):</b>
                                            <p style="padding: 0; font-size: 8px; margin: 0;"><b>Price effective for 30 days</b></p>
                                        </td>
                                        <td class="fontClass">$ @Model.Balance</td>

                                    </tr>
                                    <tr>
                                        <td class="fontClass">
                                            <b>SalesRep Name:</b>
                                        </td>
                                        <td class="fontClass">@Model.SalesRep</td>

                                    </tr>
                                    <tr>
                                        <td class="fontClass">
                                            <b>SalesRep Email:</b>
                                        </td>
                                        <td class="fontClass">@Model.SalesRepEmail</td>

                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                </tbody>

            </table>

            <div class="list-widget">
                <table style="width:100%;" cellpadding="0" cellspacing="0" class="list-table">
                    <thead>
                        <tr>
                            <th rowspan="3" style="text-align:center; vertical-align:middle!important;">List of Service</th>
                            <th rowspan="3" style="text-align:center; vertical-align:middle!important;">Description</th>
                            <th colspan="3" style="text-align: center; height: auto;">Price (in $)</th>
                            <th rowspan="3" style="text-align: center; vertical-align:middle !important;">Line Item Notes**</th>
                        </tr>
                        <tr style="">
                            <th style="height:auto; text-align: center; border-left: none !important;">Option 1</th>
                            <th style="height:auto; text-align: center;">Option 2</th>
                            <th style="height:auto; text-align: center;">Option 3</th>
                        </tr>
                        @if (!String.IsNullOrEmpty(@Model.Option1) || !String.IsNullOrEmpty(@Model.Option2) || !String.IsNullOrEmpty(@Model.Option3))
                        {
                            <tr>
                                <th style="height:auto;">
                                    @if (!String.IsNullOrEmpty(@Model.Option1))
                                    {
                                        <div style="float:left;overflow-wrap:break-word;word-wrap: break-word;">@Model.Option1</div>
                                    }
                                </th>
                                <th style="height:auto;">
                                    @if (!String.IsNullOrEmpty(@Model.Option2))
                                    {
                                        <div style="float:left;overflow-wrap:break-word;word-wrap: break-word;">@Model.Option2</div>
                                    }
                                </th>
                                <th style="height:auto;">
                                    @if (!String.IsNullOrEmpty(@Model.Option3))
                                    {
                                        <div style="float:left;overflow-wrap:break-word;word-wrap: break-word;">@Model.Option3</div>
                                    }
                                </th>
                            </tr>
                        }
                    </thead>
                    <tbody>
                        @{int i = 1;}
                        @{int notesIndex = 0;}
                        @foreach (var model2 in Model.ServiceList)
                        {
                            if (model2.BackColor != "red")
                            {
                                <tr>
                                    <td colspan="6">
                                        <b>Invoice Line#:</b> @(i++) <b> | Location: </b>@model2.LocationName <b>| Surface Type:</b> @model2.TypeOfService <b>| Material Type:</b> @model2.typeOfStoneType3 <b>| Surface Color:</b> @model2.TypeOfStoneColor <b>| Surface Finish:</b> @model2.TypeOfStoneType <b>| Service Type:</b> @model2.ServiceType
                                    </td>
                                </tr>
                                <tr>
                                    <td>@model2.ServiceIds</td>
                                    <td>@Raw(@model2.Description)</td>
                                    <td style="text-align: right;">@model2.Option1</td>
                                    <td style="text-align: right;">@model2.Option2</td>
                                    <td style="text-align: right;">@model2.Option3</td>
                                    <td>P@(++notesIndex)</td>
                                </tr>
                            }
                            if (model2.BackColor == "red")
                            {
                                <tr style="border: 10px solid;border-color:red;">
                                    <td colspan="6">
                                        <b>Invoice Line#:</b> @(i++) <b> | Location: </b>@model2.LocationName <b>| Surface Type:</b> @model2.TypeOfService <b>| Material Type:</b> @model2.typeOfStoneType3 <b>| Surface Color:</b> @model2.TypeOfStoneColor <b>| Surface Finish:</b> @model2.TypeOfStoneType <b>| Service Type:</b> @model2.ServiceType
                                    </td>
                                </tr>
                                <tr style="border: 10px solid;border-color:red;">
                                    <td style="text-decoration: line-through;">@model2.ServiceIds</td>
                                    <td style="text-decoration: line-through;">@Raw(@model2.Description)</td>
                                    <td style="text-align: right; text-decoration: line-through;">@model2.Option1</td>
                                    <td style="text-align: right; text-decoration: line-through;">@model2.Option2</td>
                                    <td style="text-align: right; text-decoration: line-through;">@model2.Option3</td>
                                    <td style="text-decoration: line-through;">P@(++notesIndex)</td>
                                </tr>
                            }
                            if (model2.SubItem.Count() > 0)
                            {
                                foreach (var subItem in model2.SubItem)
                                {
                                    if (subItem.BorderColor != "red")
                                    {
                                        <tr>
                                            <td style="border-top: none !important;">@subItem.ServiceIds.Id</td>
                                            <td style="border-top: none !important;">@Raw(@subItem.Description)</td>
                                            <td style="border-top: none !important; text-align: right;">
                                                @if (@subItem.IsMainBundle == true || @subItem.IsBundle == false)
                                                {
                                                    @subItem.Option1
                                                }
                                            </td>
                                            <td style="border-top: none !important; text-align: right;">
                                                @if (@subItem.IsMainBundle == true || @subItem.IsBundle == false)
                                                {
                                                    @subItem.Option2
                                                }
                                            </td>
                                            <td style="border-top: none !important; text-align: right;">
                                                @if (@subItem.IsMainBundle == true || @subItem.IsBundle == false)
                                                {
                                                    @subItem.Option3
                                                }
                                            </td>
                                            <td style="border-top: none !important;">P@(++notesIndex)</td>
                                        </tr>
                                    }
                                    if (subItem.BorderColor == "red")
                                    {
                                        <tr>
                                            <td style="border-top: none !important; text-decoration: line-through;">@subItem.ServiceIds.Id</td>
                                            <td style="border-top: none !important; text-decoration: line-through;">@Raw(@subItem.Description)</td>
                                            <td style="border-top: none !important; text-decoration: line-through; text-align: right;">
                                                @if (@subItem.IsMainBundle == true || @subItem.IsBundle == false)
                                                {
                                                    @subItem.Option1
                                                }
                                            </td>
                                            <td style="border-top: none !important; text-decoration: line-through; text-align: right;">
                                                @if (@subItem.IsMainBundle == true || @subItem.IsBundle == false)
                                                {
                                                    @subItem.Option2
                                                }
                                            </td>
                                            <td style="border-top: none !important; text-decoration: line-through; text-align: right;">
                                                @if (@subItem.IsMainBundle == true || @subItem.IsBundle == false)
                                                {
                                                    @subItem.Option3
                                                }
                                            </td>
                                            <td style="border-top: none !important; text-decoration: line-through;">P@(++notesIndex)</td>
                                        </tr>
                                    }
                                }
                            }

                        }
                        <tr>
                            <td><b>Total (in $)</b></td>
                            <td></td>
                            <td style="text-align: right;">
                                <b> @Model.Option1Total</b>
                            </td>
                            <td style="text-align: right;"><b>@Model.Option2Total</b></td>
                            <td style="text-align: right;"><b>@Model.Option3Total</b></td>
                            <td></td>
                        </tr>
                    </tbody>
                </table>
            </div>

            <br />
            <br />

            <div class="list-widget" style="margin-top:20px;">
                <h5 style="font-size:18px; font-weight:bold;margin-top:20px;margin-bottom:0px;line-height:10px">Measurements <span style="font-weight:100;"><i>(Total Area in ft<sup>2</sup>: @Model.TotalArea)</i></span></h5>
                <table style="width:100%;margin-top:15px;border-left: 1px solid #bec2c6;" cellpadding="0" cellspacing="0" class="table-single">
                    <thead>
                        <tr>
                            <td style="width:50px !important"><strong>Invoice Line</strong></td>
                            <td style="width:50px !important"><strong>Length(ft.)</strong></td>
                            <td style="width:50px !important"><strong>Width(ft.)</strong></td>
                            <td style="width:50px !important"><strong>Area(ft<sup>2</sup>)</strong></td>
                            <td><strong>Description</strong></td>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var model2 in Model.EstimateInvoiceDimensionTables)
                        {
                            if (@model2.DimensionList.Count() > 0)
                            {
                                <tr>
                                    <td rowspan=@model2.DimensionList.Count() style="border-bottom: 1px solid #bec2c6">@model2.InvoiceLine</td>
                                    <td>@model2.DimensionList[0].Length</td>
                                    <td>@model2.DimensionList[0].Width</td>
                                    <td>@model2.DimensionList[0].Area</td>
                                    <td>@model2.DimensionList[0].Description</td>
                                </tr>
                                foreach (var measurementList in model2.DimensionList)
                                {
                                    if (measurementList == model2.DimensionList[0])
                                    {
                                        continue;
                                    }
                                    <tr>
                                        <td>@measurementList.Length</td>
                                        <td>@measurementList.Width</td>
                                        <td>@measurementList.Area</td>
                                        <td>@measurementList.Description</td>
                                    </tr>
                                }
                            }
                            else
                            {
                                <tr>
                                    <td style="border-bottom: 1px solid #bec2c6">@model2.InvoiceLine</td>
                                    <td>0</td>
                                    <td>0</td>
                                    <td>0</td>
                                    <td></td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>


            <br />
            <br />
            @if (Model.NotesCount > 0)
            {
                <div class="list-widget" style="margin-top:20px;">
                    <h5 style="font-size:18px; font-weight:bold;margin-top:20px;margin-bottom:0px;line-height:10px">Line Item Notes**</h5>
                    <table style="width:100%;margin-top:15px;border-left: 1px solid #bec2c6;" cellpadding="0" cellspacing="0" class="table-single">
                        <thead>
                            <tr>
                                <td style="width:30px !important">Variable</td>
                                <td style="width:150px !important">Notes</td>
                            </tr>
                        </thead>
                        <tbody>
                            @{int pCount = 0;}
                            @foreach (var model2 in Model.ServiceList)
                            {
                                pCount = ++pCount;
                                if (model2.Notes != "")
                                {
                                    <tr>
                                        <td style="width:20px !important">P@(pCount)</td>
                                        <td style="width:150px !important;word-break: break-all;">
                                            @model2.Notes
                                        </td>
                                    </tr>
                                }
                                if (model2.SubItem.Count() > 0)
                                {
                                    foreach (var subItem in model2.SubItem)
                                    {
                                        pCount = ++pCount;
                                        if (!string.IsNullOrEmpty(subItem.PriceNotes))
                                        {
                                            <tr>
                                                <td style="width:20px !important">P@(pCount)</td>
                                                <td style="width:150px !important;word-break: break-all;">
                                                    @subItem.PriceNotes
                                                </td>
                                            </tr>
                                        }
                                    }
                                }
                            }
                        </tbody>
                    </table>
                </div>
            }


            @if (@Model.InvoiceNote != "" && @Model.InvoiceNote != null)
            {
                <div class="list-widget" style="margin-top:30px;width:800px !important">
                    <h5 style="font-size:18px; font-weight:bold;margin-top:20px;margin-bottom:10px;line-height:10px;">Invoice Notes</h5>
                    <div style="border: 1px solid #bec2c6;width:800px !important">
                        <p style="margin:0;margin-top:20px;margin-bottom:20px;padding:4px;overflow-wrap:break-word !important;word-wrap: break-word;">
                            @Raw(Model.InvoiceNote)
                        </p>
                    </div>
                </div>
            }

            @if (@Model.EstimateNote != "" && @Model.EstimateNote != null)
            {
                <div class="list-widget" style="margin-top:30px;width:800px">
                    <h5 style="font-size:18px; font-weight:bold;margin-top:20px;margin-bottom:10px;line-height:10px;">General Notes</h5>
                    <div style="border: 1px solid #bec2c6;width:800px !important">
                        <p style="margin:0;margin-top:20px;margin-bottom:20px;padding:4px;overflow-wrap:break-word;word-wrap: break-word;">
                            @Raw(Model.EstimateNote)
                        </p>
                    </div>
                </div>
            }

            <div class="list-widget" style="margin-top: 20px; padding: 10px; border: 1px solid #bec2c6;">
                <p style="margin:0;">
                    <b> To our customer,</b> we are asking permission to take picture before and after of the work in order to document to initial and final conditions.
                    I accept this agreement and understand the terms and conditions provided on the back of this agreement.
                </p>
                <table style="width:100%;" cellpadding="0" cellspacing="0">
                    <tbody>
                        <tr>
                            <td style="width:180px!important"><b>Sales Representative</b></td>
                            <td>@Model.SalesRep</td>
                            <td><b> Customer Signature: <span style="font-size:10px">At time of sale</span>:</b></td>


                            @if (@Model.CustomerSignature != "" && @Model.CustomerSignature != null)
                            {
                                <td style="max-width:30px !important;width:10px!important"><img style="margin:auto; height:100px;width:140px" src="@Model.CustomerSignature" /></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:40px !important;width:10px!important"></td>
                                <td style="max-width:40px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>

                            }
                            @if (@Model.CustomerSignature == "" || @Model.CustomerSignature == null)
                            {
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                            }

                            <td><b>Date:</b></td>
                            <td style="width:80px !important">@Model.SignDateTime</td>

                        </tr>
                    </tbody>

                </table>

            </div>
            <div class="list-widget" style="font-size: 10px; border-left: 1px solid #bec2c6; padding: 10px; border-right: 1px solid #bec2c6; border-bottom: 1px solid #bec2c6;">
                Acceptance by customer of service provided by MARBLELIFE shall be deemed to constitute assent to MARBLELIFE's terms and conditions set forth herein.
                MARBLELIFE reserve the right to improve its propietry process at any time without notice. Guarantee is valid only if payment for services are current. A late charge of 1.5% per month will be imposed on past due accounts.
            </div>

            <div>
                <table style="width:100%;" cellpadding="0" cellspacing="0">
                    <tbody>
                        <tr>
                            <td style="width:180px!important"><b>Technician</b></td>
                            <td>@Model.TechName</td>
                            <td><b> Customer Signature: <span style="font-size:10px">On Completion</span>:</b></td>


                            @if (@Model.CustomerPostSignature != "" && @Model.CustomerPostSignature != null)
                            {
                                <td style="max-width:30px !important;width:10px!important"><img style="margin:auto; height:100px;width:140px" src="@Model.CustomerPostSignature" /></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
                                <td style="max-width:10px !important;width:10px!important"></td>
